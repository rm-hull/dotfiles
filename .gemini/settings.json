{
  "ide": {
    "hasSeenNudge": true,
    "enabled": true
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "ui": {
    "accessibility": {
      "disableLoadingPhrases": true
    },
    "footer": {
      "hideContextPercentage": false
    }
  },
  "general": {
    "previewFeatures": true
  },
  "output": {
    "format": "text"
  },
  "tools": {
    "shell": {
      "showColor": true
    }
  },
  "mcpServers": {
    "chakra-ui": {
      "command": "npx",
      "args": [
        "-y",
        "@chakra-ui/react-mcp"
      ],
      "trust": true
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "trust": true
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ],
      "trust": true
    }
  }
}
